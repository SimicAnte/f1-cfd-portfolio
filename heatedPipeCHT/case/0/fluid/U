FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 1.2093); /* velocity for Re=40 000 */

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 1.2093);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 1.2093);
        value           uniform (0 0 1.2093);
    }
    fluid_to_solid
    {
        type            noSlip;
    }
}

// ************************************************************************* //
FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 10; 

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 10; 
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 10;
        value           uniform 10;
    }

    // Wall function for omega
    fluid_to_solid
    {
        type            omegaWallFunction;
        value           uniform 10;
    }
}
// ************************************************************************* //

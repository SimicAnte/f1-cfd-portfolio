FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 1.7);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 1.7);
    }

    outlet
    {
        type            zeroGradient;
    }

    wall_fluid
    {
        type            noSlip;
    }

    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //